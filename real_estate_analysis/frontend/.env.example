# Application
REACT_APP_NAME=Real Estate Analysis Platform
REACT_APP_ENV=development
REACT_APP_VERSION=$npm_package_version

# API
REACT_APP_API_BASE_URL=http://localhost:8000/api/v1

# Authentication
REACT_APP_AUTH_STORAGE_KEY=real_estate_auth
REACT_APP_TOKEN_STORAGE_KEY=real_estate_token
REACT_APP_REFRESH_TOKEN_STORAGE_KEY=real_estate_refresh_token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_EMAIL_REPORTS=true
REACT_APP_ENABLE_MARKET_INSIGHTS=true

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Analytics
REACT_APP_GA_TRACKING_ID=your-ga-tracking-id
REACT_APP_GA_ENABLED=true

# Monitoring
REACT_APP_SENTRY_ENABLED=true

# Theme
REACT_APP_THEME=light
REACT_APP_PRIMARY_COLOR=#1976d2
REACT_APP_SECONDARY_COLOR=#dc004e

# Cache
REACT_APP_CACHE_TTL=300
REACT_APP_CACHE_PREFIX=real_estate_

# Localization
REACT_APP_DEFAULT_LOCALE=en
REACT_APP_SUPPORTED_LOCALES=["en", "es", "fr", "de"]

# Date Format
REACT_APP_DATE_FORMAT=YYYY-MM-DD
REACT_APP_TIME_FORMAT=HH:mm:ss

# Currency
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_CURRENCY_FORMAT=USD

# Pagination
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_MAX_PAGE_SIZE=100

# File Upload
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_SUPPORTED_FILE_TYPES=image/jpeg,image/png,image/gif

# Error Handling
REACT_APP_ERROR_BOUNDARY_ENABLED=true
REACT_APP_ERROR_REPORTING_ENABLED=true

# Performance
REACT_APP_ENABLE_CODE_SPLITTING=true
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_PREFETCHING=true

# Development
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_ENABLE_HOT_RELOAD=true
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_LOG_LEVEL=debug 