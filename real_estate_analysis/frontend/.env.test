# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api/v1

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=false

# Application Settings
REACT_APP_MAX_FILE_SIZE=5242880  # 5MB in bytes
REACT_APP_SUPPORTED_FILE_TYPES=image/jpeg,image/png,image/gif

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your_test_google_maps_api_key
REACT_APP_SENTRY_DSN=your_test_sentry_dsn

# Test Settings
REACT_APP_ENABLE_DEV_TOOLS=false
REACT_APP_LOG_LEVEL=error
REACT_APP_ENABLE_HOT_RELOAD=false
REACT_APP_ENABLE_SOURCE_MAPS=false 